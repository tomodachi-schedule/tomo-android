<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_quest"
        android:name="com.example.tomodachischedule.ui.quest.QuestsFragment"
        android:label="@string/title_quests"
        tools:layout="@layout/fragment_quest" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.tomodachischedule.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/activity_main" />

    <fragment
        android:id="@+id/navigation_store"
        android:name="com.example.tomodachischedule.ui.dashboard.DashboardFragment"
        android:label="@string/title_store"
        tools:layout="@layout/fragment_store" />

    <fragment
        android:id="@+id/navigation_prefs"
        android:name="com.example.tomodachischedule.ui.quest.QuestsFragment"
        android:label="@string/title_preferences"
        tools:layout="@layout/fragment_prefs" />
</navigation>